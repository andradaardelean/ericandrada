      </TableView>
      <Button fx:id="buttonAccept" layoutX="426.0" layoutY="121.0" mnemonicParsing="false" onAction="#onAcceptRequest" prefHeight="50.0" prefWidth="130.0" style="-fx-background-color: pink;" text="Accept Request">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="buttonDecline" layoutX="425.0" layoutY="221.0" mnemonicParsing="false" onAction="#onDeclineRequest" prefHeight="50.0" prefWidth="130.0" style="-fx-background-color: pink;" text="Decline Request">
